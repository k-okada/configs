#!/bin/sh
#


# update x resources db
xrdb -load $HOME/.Xresources

# set wallpaper
sh $HOME/.fehbg &

# numlock on
#/usr/bin/numlockx &

# hide mouse cursor
#/usr/bin/unclutter -visible -noevents &

# compositing
/usr/bin/compton &

# date/time
/usr/local/bin/dss &

aplay ~/sys/snd/psone.wav &

# start dwm
exec /usr/local/bin/dwm

